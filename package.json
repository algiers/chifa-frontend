{
  "name": "chifa-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:prod": "powershell -Command \"$env:NODE_ENV='production'; next build\"",
    "start": "next start",
    "start:prod": "powershell -Command \"$env:NODE_ENV='production'; next start\"",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "powershell -Command \"if (Test-Path '.next') { Remove-Item -Path '.next' -Recurse -Force }; if (Test-Path 'node_modules\\.cache') { Remove-Item -Path 'node_modules\\.cache' -Recurse -Force }\"",
    "clean:all": "powershell -Command \"if (Test-Path '.next') { Remove-Item -Path '.next' -Recurse -Force }; if (Test-Path 'node_modules') { Remove-Item -Path 'node_modules' -Recurse -Force }; if (Test-Path 'package-lock.json') { Remove-Item 'package-lock.json' -Force }\" && npm install",
    "env:validate": "node scripts/validate-env.js",
    "env:generate-secret": "node scripts/generate-auth-secret.js",
    "env:setup": "powershell -Command \"Copy-Item -Path '.env.chifa-vercel' -Destination '.env.local'\" && echo Fichier .env.local cree! Adaptez les valeurs puis executez: npm run env:validate"
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0-beta.1",
