    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.2.4",
